<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.damaznia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DAmazônia">
        <activity android:name=".HomeActivity"></activity>
        <activity
            android:name=".CadastrarActivity"
            android:label="Cadastrar" />
        <activity
            android:name=".EntrarActivity"
            android:label="Entrar" />
        <activity
            android:name=".OnBoarding3Activity"
            android:theme="@style/Theme.DAmazôniaNoActionBar"/>
        <activity
            android:name=".OnBoarding2Activity"
            android:theme="@style/Theme.DAmazôniaNoActionBar"/>
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/Theme.DAmazôniaNoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.DAmazôniaNoActionBar"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>